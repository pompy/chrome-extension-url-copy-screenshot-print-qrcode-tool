{  "manifest_version": 2,  "name": "Copy Url",    "description": "Copies Url To Clipboard",  "version": "0.1",  "background": {  "persistent": false,    "scripts": ["background.js"]  },  "content_scripts": [    {      "matches": [        "<all_urls>"      ],      "js": ["content.js"]    }  ],  "browser_action": {  "default_icon": "icon.png",    "default_popup": "background.html"},"permissions": [     "tabs",  "clipboardRead",  "clipboardWrite",  "activeTab",  "pageCapture",        "downloads",		"bookmarks"]}